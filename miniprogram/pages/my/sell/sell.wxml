<view class="sell-container">
  <view class="page-title">
    <text class="title">我卖出的商品</text>
  </view>

  <!-- 如果卖出的商品列表为空，显示提示 -->
  <view wx:if="{{sellGoodsList.length === 0}}" class="no-goods">
    <text>你还没有卖出商品</text>
  </view>

  <!-- 使用 goods-list 组件展示卖出的商品 -->
  <goods-list goodsList="{{sellGoodsList}}" />
</view>
